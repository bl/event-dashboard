{
  "name": "event-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "connect-pg-simple": "^4.2.1",
    "express": "^4.15.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "googleapis": "^22.1.0",
    "moment": "^2.18.1",
    "node-jsx": "^0.13.3",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.1",
    "sequelize": "^4.13.5"
  },
  "scripts": {
    "start": "npm run build && node ./build/server.js",
    "build": "webpack --config config/webpack.config.js",
    "db:build": "node ./db/init.js",
    "db:build:force": "node ./db/init.js --force",
    "db:init": "pg_ctl -D /usr/local/var/postgres initdb",
    "db:start": "pg_ctl start -l db/logfile -D /usr/local/var/postgres",
    "db:stop": "pg_ctl stop -D /usr/local/var/postgres"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  }
}
